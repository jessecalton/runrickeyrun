{
  "manifest_version": 2,
  "name": "Run Rickey Run",
  "short_name": "Run Rickey",
  "version": "0.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "baseball.png"
  }
}